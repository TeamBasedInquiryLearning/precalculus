    <!-- {{#zero}} -->
    <knowl>
        <content>
            <p>
                Find the {{name1}} of the rational function <m>\displaystyle f(x)=\frac{ {{num}} }{ {{denom}}  }</m>.
            </p>
        </content>

        <outtro>
            <p>
                <m>f(x)=\displaystyle \frac{ {{num}} }{ {{denom}} }</m> has no {{name1}}.
            </p>
        </outtro>
        
    </knowl> 
    
    <knowl>
        <content>
            <p>
                Find the <m>y</m>-intercept of <m>y=\displaystyle \frac{ {{num}} }{ {{denom}} }</m>.
            </p>
        </content>

        <outtro>
            <p>
                The <m>y</m>-intercept of <m>y=\displaystyle \frac{ {{num}} }{ {{denom}} }</m> is <m>\left(0,{{yint}} \right)</m>.
            </p>
        </outtro>
        
    </knowl> 
    
    <knowl>
        <content>
            <p>
                Find any horizontal asymptotes of <m>y=\displaystyle \frac{ {{num}} }{ {{denom}} }</m>.
            </p>
        </content>

        <outtro>
            <p>
                <m>y={{horizontal}}</m>.
            </p>
        </outtro>
    </knowl>
    
    <knowl>
        <content>
            <p>
                Find any vertical asymptotes of <m>y=\displaystyle \frac{ {{num}} }{ {{denom}} }</m>.
            </p>
        </content>

        <outtro>
            <p>
                <m>x={{vertical}}</m>.
            </p>
        </outtro>
    </knowl>
    
    <knowl>
        <content>
            <p>
                Find any holes of <m>y=\displaystyle \frac{ {{num}} }{ {{denom}} }</m>.
            </p>
        </content>

        <outtro>
            <p>
                There is a hole at <m>\left({{holex}}, {{holey}} \right)</m>.
            </p>
        </outtro>
    </knowl>
    
    <knowl>
        <content>
            <p>
                Sketch <m>y=\displaystyle \frac{ {{num}} }{ {{denom}} }</m>.
            </p>
            
        </content>
        <outtro>
            <p>
                <image
                source="assets/PR6/generated/{{__seed__}}/plot.png"
                description="The plot of y=f(x), along with special points."/>
            </p>
        </outtro>
    </knowl>
