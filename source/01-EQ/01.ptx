          P+= line([(-4,0),(-3,0)],color="#0088ff", thickness=3)
          P += text("[", (-4,0), color="#0088ff", fontsize=16)
          P += text(")", (-3,0), color="#0088ff", fontsize=16)
          P.axes(False)
          P
